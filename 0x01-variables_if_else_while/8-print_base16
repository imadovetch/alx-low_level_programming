#include <stdio.h>

/**
 * main - the possible different combinations of three digits
 * followed by a new line
 * Description: description
 * Return: Always 0 (Success)
 */
int main(void)
{
	int n;
	char x;

	for (n = 48; n < 58; n++)
	{
		putchar(n);
	}
	for (x = 'a'; x <= 'f'; x++)
	{
		putchar(x);
	}
	putchar('\n');
	return (0);
}
